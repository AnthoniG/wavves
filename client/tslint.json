{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "no-console": false,
    "ordered-imports": false,
    "interface-name": false,
    "no-implicit-any": false,
    "variable-name": false
  }
}

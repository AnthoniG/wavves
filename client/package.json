{
  "name": "client",
  "private": true,
  "dependencies": {
    "@reach/router": "^1.1.1",
    "node-sass-chokidar": "^1.3.0",
    "react": "^16.4.1",
    "react-copy-write": "^0.7.0",
    "react-dom": "^16.4.1",
    "react-loadable": "^5.5.0",
    "react-scripts-ts": "2.16.0",
    "urql": "^0.2.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/ --include-path src/styling",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --include-path src/styling --watch --recursive",
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build && yarn build-css && sw-precache --root='build/' --config config/sw.js ",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/jest": "^23.1.4",
    "@types/node": "^10.5.1",
    "@types/reach__router": "^1.0.0",
    "@types/react": "^16.4.6",
    "@types/react-copy-write": "^0.7.0",
    "@types/react-dom": "^16.0.6",
    "sw-precache": "^5.2.1",
    "typescript": "^3.0.0-rc"
  },
  "proxy": "http://localhost:3004/"
}
